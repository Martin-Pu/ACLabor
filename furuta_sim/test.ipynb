   "source": [
    "import sympy as sp\n",
    "import numpy as np\n",
    "\n",
    "x, m, g = sp.symbols('x, m, g')\n",
    "\n",
    "expr = m*g + g\n",
    "\n",
    "# Substitute constants first\n",
    "expr_const = expr.subs({m: 2.0})\n",
    "\n",
    "f = sp.lambdify(x, expr_const, \"numpy\")\n",
    "\n",
    "print(f(4))  # 2*2^2 + sin(3*1)\n"
   ]
  },
